<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dw.applebuy">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <!-- 地图 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name="com.wlj.base.util.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service
            android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote"></service>
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="r5AATPw9f0soBFBFXUGp6ib5" />

        <activity android:name=".ui.Loading">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.loginreg.LoginActivity" />
        <activity android:name=".ui.loginreg.RegisterActivity" />
        <activity android:name=".ui.loginreg.ForgetPswActivity" />
        <activity android:name=".ui.home.shoppingmanage.ShoppingManagerActivity" />
        <activity
            android:name=".ui.home.ordermanage.ItemListActivity"
            android:label="@string/title_item_list"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ui.home.ordermanage.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".ui.home.ordermanage.ItemListActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.home.ordermanage.ItemListActivity" />
        </activity>
        <activity android:name=".ui.home.shoppingmanage.youhui.YouHuiManagerActivity" />
        <activity android:name=".ui.home.shoppingmanage.youhui.add.YouHuiAddActivity" />
        <activity android:name=".ui.home.shoppingmanage.youhui.add.YouHuiTypeActivity" />
        <activity android:name=".ui.home.shoppingmanage.youhui.add.YouHuiTimeActivity" />
        <activity android:name=".ui.loginreg.ChangePhoneActivity" />
        <activity android:name=".ui.set.AboutUsActivity" />
        <activity android:name=".ui.home.shoppingmanage.album.AlbumActivity" />
        <activity android:name=".ui.home.shoppingmanage.album.UpLoadImageActivity" />
        <activity android:name=".ui.home.shoppingmanage.data.DataActivity" />
        <activity android:name=".ui.home.shoppingmanage.data.RegionActivity" />
        <activity android:name=".ui.home.shoppingmanage.data.MapActivity"></activity>
        <activity android:name=".ui.home.shoppingmanage.data.BusinessScopeActivity"></activity>
    </application>

</manifest>